<?xml version="1.0"?>
<rdf:RDF xmlns="http://isae.fr/iqas/qoo-ontology#"
     xml:base="http://isae.fr/iqas/qoo-ontology"
     xmlns:iot-lite="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#"
     xmlns:qu="http://purl.org/NET/ssnx/qu/qu#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://creativecommons.org/ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:m3-lite="http://purl.org/iot/vocab/m3-lite#"
     xmlns:ssn="http://purl.oclc.org/NET/ssnx/ssn#"
     xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
     xmlns:qoo="http://isae.fr/iqas/qoo-ontology#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:qu-rec20="http://purl.org/NET/ssnx/qu/qu-rec20#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://isae.fr/iqas/qoo-ontology#">
        <rdfs:label xml:lang="en">qoo-ontology</rdfs:label>
        <dc:description xml:lang="en">The qoo-ontology is a lightweight ontology based on IoT-Lite and SSN to describe and characterized sensors, observations and Quality of Observation (QoO).</dc:description>
        <owl:versionInfo xml:lang="en">1.0.0</owl:versionInfo>
        <dc:creator xml:lang="en">Antoine Auger</dc:creator>
        <dc:title xml:lang="en">The QoO Ontology (QoOnto)</dc:title>
        <dc:date xml:lang="en">January 2017</dc:date>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#altRelative -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#altRelative">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#relativeLocation -->

    <owl:AnnotationProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#relativeLocation">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#alt -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#alt">
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#lat -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#lat">
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#long -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#long">
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://isae.fr/iqas/qoo-ontology#observationLevel -->

    <rdfs:Datatype rdf:about="http://isae.fr/iqas/qoo-ontology#observationLevel">
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>INFORMATION</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>KNOWLEDGE</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>RAW DATA</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>
    


    <!-- http://isae.fr/iqas/qoo-ontology#sensorState -->

    <rdfs:Datatype rdf:about="http://isae.fr/iqas/qoo-ontology#sensorState">
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>CONNECTED</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>DISCONNECTED</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>IDLE</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>SENSING</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://isae.fr/iqas/qoo-ontology#belongsTo -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#belongsTo">
        <owl:inverseOf rdf:resource="http://isae.fr/iqas/qoo-ontology#contains"/>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Device"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#canBeRetrievedThrough -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#canBeRetrievedThrough">
        <owl:inverseOf rdf:resource="http://isae.fr/iqas/qoo-ontology#givesAccessTo"/>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Property"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#cannotBeAppliedAfter -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#cannotBeAppliedAfter">
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#contains -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#contains">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Device"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#decreases -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#decreases">
        <owl:inverseOf rdf:resource="http://isae.fr/iqas/qoo-ontology#isDecreasedBy"/>
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#QualityofObservation"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#exposedBy -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#exposedBy">
        <owl:inverseOf rdf:resource="http://isae.fr/iqas/qoo-ontology#exposes"/>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Platform"/>
        <rdfs:comment xml:lang="en">A service is exposed by the platform.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#exposes -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#exposes">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Platform"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
        <rdfs:comment xml:lang="en">The platform exposes some service to query the different Sensors.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#givesAccessTo -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#givesAccessTo">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Property"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#hasMetadata -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#hasMetadata">
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Attribute"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#ObsMetadata"/>
        <rdfs:comment>Links any concept with metadata about that concept.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#hasParams -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#hasParams">
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#Parameter"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#hasQuality -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#hasQuality">
        <owl:inverseOf rdf:resource="http://isae.fr/iqas/qoo-ontology#usedToCharacterized"/>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#ObservationValue"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#Attribute"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#increases -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#increases">
        <owl:inverseOf rdf:resource="http://isae.fr/iqas/qoo-ontology#isIncreasedBy"/>
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#QualityofObservation"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#isDecreasedBy -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#isDecreasedBy">
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#QualityofObservation"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#isIncreasedBy -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#isIncreasedBy">
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#QualityofObservation"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#isProvidedBy -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#isProvidedBy">
        <owl:inverseOf rdf:resource="http://isae.fr/iqas/qoo-ontology#provides"/>
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Platform"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#isRelatedTo -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#isRelatedTo">
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#SensingRate"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#isSupportedBy -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#isSupportedBy">
        <owl:inverseOf rdf:resource="http://isae.fr/iqas/qoo-ontology#supports"/>
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Operator"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#provides -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#provides">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Platform"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#supports -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#supports">
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Mechanism"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#Operator"/>
    </owl:ObjectProperty>
    


    <!-- http://isae.fr/iqas/qoo-ontology#usedToCharacterized -->

    <owl:ObjectProperty rdf:about="http://isae.fr/iqas/qoo-ontology#usedToCharacterized">
        <rdfs:domain rdf:resource="http://isae.fr/iqas/qoo-ontology#Attribute"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#ObservationValue"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasCoverage -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasCoverage">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#SensingDevice"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Coverage"/>
        <rdfs:comment>Links the devices with their coverages.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasCurrently -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasCurrently">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
        <rdfs:range rdf:resource="http://isae.fr/iqas/qoo-ontology#CurrentSetting"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasQuantityKind -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasQuantityKind">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#ObservationValue"/>
        <rdfs:range rdf:resource="http://purl.org/NET/ssnx/qu/qu#QuantityKind"/>
        <rdfs:comment>Links a sensor or an attribute with the quantity  kind it measures (e.g. A sensor -sensor1- measures temperature: sensor1 hasQuantityKind temperature).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasSensingDevice -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasSensingDevice">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#SensingDevice"/>
        <rdfs:comment>Links a sensor with a sensing device the same way as SSN.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasUnit -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasUnit">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#ObservationValue"/>
        <rdfs:range rdf:resource="http://purl.org/NET/ssnx/qu/qu#Unit"/>
        <rdfs:comment>Links the sensor with the units of the quantity kind it measures (e.g. A sensor -sensor1- measures temperature in Celsius: senso1 hasUnit celsius).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#isSubSystemOf -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#isSubSystemOf">
        <owl:inverseOf rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#hasSubSystem"/>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#System"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#System"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#attachedSystem -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#attachedSystem">
        <owl:inverseOf rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#onPlatform"/>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Platform"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#System"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#featureOfInterest -->

    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#featureOfInterest">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Observation"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest"/>
    </rdf:Description>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#forProperty -->

    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#forProperty">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementCapability"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Property"/>
    </rdf:Description>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#hasMeasurementCapability -->

    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#hasMeasurementCapability">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementCapability"/>
    </rdf:Description>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#hasMeasurementProperty -->

    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#hasMeasurementProperty">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementCapability"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementProperty"/>
    </rdf:Description>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#hasProperty -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#hasProperty">
        <owl:inverseOf rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#isPropertyOf"/>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Property"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#hasSubSystem -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#hasSubSystem">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#System"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#System"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#hasValue -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#hasValue">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#SensorOutput"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#ObservationValue"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#isProducedBy -->

    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#isProducedBy">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#SensorOutput"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
    </rdf:Description>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#isPropertyOf -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#isPropertyOf">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Property"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#madeObservation -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#madeObservation">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
