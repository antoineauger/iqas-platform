{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "iot-lite": "http://purl.oclc.org/NET/UNIS/fiware/iot-lite#",
    "ssn": "http://purl.oclc.org/NET/ssnx/ssn#",
    "qoo": "http://isae.fr/iqas/qoo-ontology#",
    "geo": "http://www.w3.org/2003/01/geo/wgs84_pos#"
  },
  "@graph": [
    {
      "@id": "ssn:iQAS",
      "@type": "ssn:Platform",
      "ssn:provides": [
        {
          "@id": "ssn:FILTERING",
          "@type": "qoo:Mechanism"
        }
      ],
      "qoo:exposes": [
        {
          "@id": "iot-lite:serv_sensor01_temp",
          "@type": "iot-lite:Service",
          "iot-lite:interfaceType": "REST",
          "iot-lite:endpoint": "http://127.0.0.1:9091/s01",
          "iot-lite:interfaceDescription": "First service to get temperature measurements from s01."
        },
        {
          "@id": "iot-lite:serv_sensor01_visib",
          "@type": "iot-lite:Service",
          "iot-lite:interfaceType": "REST",
          "iot-lite:endpoint": "http://127.0.0.1:9092/s01",
          "iot-lite:interfaceDescription": "Second service to get visibility measurements from s01."
        },
        {
          "@id": "iot-lite:serv_sensor02_visib",
          "@type": "iot-lite:Service",
          "iot-lite:interfaceType": "REST",
          "iot-lite:endpoint": "http://127.0.0.1:9093/s02",
          "iot-lite:interfaceDescription": "Third service to get visibility measurements from s02."
        }
      ],
      "ssn:attachedSystem": [
        {
          "@id": "ssn:sensor01",
          "@type": "ssn:SensingDevice",
          "iot-lite:id": "sensor01",
          "geo:location": {
            "@id": "qoo:loc_sensor01",
            "@type": "geo:Point",
            "geo:lat": "43.59543567",
            "geo:long": "1.45255566",
            "geo:alt": "145",
            "iot-lite:relativeLocation": "Toulouse",
            "iot-lite:altRelative": "1"
          },
          "ssn:madeObservation": [
            {
              "@id": "ssn:obs_sensor01",
              "@type": "ssn:Observation",
              "ssn:FeatureOfInterest": {
                "@id": "ssn:environment",
                "@type": "ssn:FeatureOfInterest",
                "ssn:hasProperty": [
                  {
                    "@id": "ssn:temperature",
                    "@type": "ssn:Property",
                    "qoo:canBeRetrievedThrough": {
                      "@id": "iot-lite:serv_sensor01_temp",
                      "@type": "iot-lite:Service"
                    }
                  },
                  {
                    "@id": "ssn:visibility",
                    "@type": "ssn:Property",
                    "qoo:canBeRetrievedThrough": {
                      "@id": "iot-lite:serv_sensor01_visib",
                      "@type": "iot-lite:Service"
                    }
                  }
                ]
              }
            }
          ],
          "ssn:hasMeasurementCapability": [
            {
              "@id": "ssn:capa_temp_sensor01",
              "@type": "ssn:MeasurementCapability",
              "ssn:hasMeasurementProperty": [
                {
                  "@id": "ssn:range_temp_sensor01",
                  "@type": "ssn:MeasurementRange",
                  "qoo:hasMinValue": "-50",
                  "qoo:hasMaxValue": "50"
                }
              ],
              "ssn:forProperty": {
                "@id": "ssn:temperature",
                "@type": "ssn:Property"
              }
            },
            {
              "@id": "ssn:capa_visib_sensor01",
              "@type": "ssn:MeasurementCapability",
              "ssn:hasMeasurementProperty": [
                {
                  "@id": "ssn:range_visib_sensor01",
                  "@type": "ssn:MeasurementRange",
                  "qoo:hasMinValue": "0",
                  "qoo:hasMaxValue": "+INF"
                }
              ],
              "ssn:forProperty": {
                "@id": "ssn:visibility",
                "@type": "ssn:Property"
              }
            }
          ]
        },
        {
          "@id": "ssn:sensor02",
          "@type": "ssn:SensingDevice",
          "iot-lite:id": "sensor02",
          "geo:location": {
            "@id": "qoo:loc_sensor02",
            "@type": "geo:Point",
            "geo:lat": "43.53101809",
            "geo:long": "1.40158296",
            "geo:alt": "152",
            "iot-lite:relativeLocation": "Portet-sur-Garonne",
            "iot-lite:altRelative": "1"
          },
          "ssn:madeObservation": [
            {
              "@id": "ssn:obs_sensor02",
              "@type": "ssn:Observation",
              "ssn:FeatureOfInterest": {
                "@id": "ssn:environment",
                "@type": "ssn:FeatureOfInterest",
                "ssn:hasProperty": [
                  {
                    "@id": "ssn:visibility",
                    "@type": "ssn:Property",
                    "qoo:canBeRetrievedThrough": {
                      "@id": "iot-lite:serv_sensor02_visib",
                      "@type": "iot-lite:Service"
                    }
                  }
                ]
              }
            }
          ],
          "ssn:hasMeasurementCapability": [
            {
              "@id": "ssn:capa_visib_sensor02",
              "@type": "ssn:MeasurementCapability",
              "ssn:hasMeasurementProperty": [
                {
                  "@id": "ssn:range_visib_sensor02",
                  "@type": "ssn:MeasurementRange",
                  "qoo:hasMinValue": "0",
                  "qoo:hasMaxValue": "+INF"
                }
              ],
              "ssn:forProperty": {
                "@id": "ssn:visibility",
                "@type": "ssn:Property"
              }
            }
          ]
        }
      ]
    }
  ]
}