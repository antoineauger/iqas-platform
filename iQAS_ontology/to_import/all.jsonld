{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "iot-lite": "http://purl.oclc.org/NET/UNIS/fiware/iot-lite#",
    "ssn": "http://purl.oclc.org/NET/ssnx/ssn#",
    "qoo": "http://isae.fr/iqas/qoo-ontology#",
    "geo": "http://www.w3.org/2003/01/geo/wgs84_pos#"
  },
  "@graph": [
    {
      "@id": "qoo:temperature",
      "@type": "ssn:Property"
    },
    {
      "@id": "qoo:visibility",
      "@type": "ssn:Property"
    },
    { 
      "@id": "qoo:loc_toulouse",
      "@type": "geo:Point",
      "geo:lat": "43.601176",
      "geo:long": "1.439363",
      "geo:alt": "0",
      "iot-lite:relativeLocation": "Toulouse",
      "iot-lite:altRelative": "0"
    },
    { 
      "@id": "qoo:loc_portet",
      "@type": "geo:Point",
      "geo:lat": "43.53101809",
      "geo:long": "1.40158296",
      "geo:alt": "152",
      "iot-lite:relativeLocation": "Chullanka - Portet-sur-Garonne",
      "iot-lite:altRelative": "0",
      "qoo:isInTheAreaOf": {
        "@id": "qoo:loc_toulouse"
      }
    },
    { 
      "@id": "qoo:loc_jardin",
      "@type": "geo:Point",
      "geo:lat": "43.59543567",
      "geo:long": "1.45255566",
      "geo:alt": "145",
      "iot-lite:relativeLocation": "Jardin des plantes - Toulouse",
      "iot-lite:altRelative": "0",
      "qoo:isInTheAreaOf": {
        "@id": "qoo:loc_toulouse"
      }
    },
    {
      "@id": "qoo:serv_sensor01_temp",
      "@type": "iot-lite:Service",
      "iot-lite:interfaceType": "REST",
      "iot-lite:endpoint": "http://127.0.0.1:9091/s01",
      "iot-lite:interfaceDescription": "First service to get temperature measurements from s01."
    },
    {
      "@id": "qoo:serv_sensor02_visib",
      "@type": "iot-lite:Service",
      "iot-lite:interfaceType": "REST",
      "iot-lite:endpoint": "http://127.0.0.1:9092/s02",
      "iot-lite:interfaceDescription": "Second service to get visibility measurements from s02."
    },
    {
      "@id": "qoo:serv_sensor03_visib",
      "@type": "iot-lite:Service",
      "iot-lite:interfaceType": "REST",
      "iot-lite:endpoint": "http://127.0.0.1:9093/s03",
      "iot-lite:interfaceDescription": "Third service to get visibility measurements from s03."
    },
    {
      "@id": "qoo:sensor01",
      "@type": "ssn:Sensor",
      "iot-lite:id": "sensor01",
      "geo:location": {
        "@id": "qoo:loc_portet"
      },
      "qoo:sensorStateValue": "CONNECTED",
      "ssn:madeObservation": [
        {
          "@id": "qoo:obs_sensor01",
          "ssn:observedProperty": [
            {
              "@id": "qoo:temperature",
              "@type": "ssn:Property",
              "qoo:canBeRetrievedThrough": {
                "@id": "qoo:serv_sensor01_temp"
              }
            }
          ]   
        }
      ],
      "iot-lite:exposedBy": [
        {
          "@id": "qoo:serv_sensor01_temp"
        }
      ],
      "ssn:hasMeasurementCapability": [
        {
          "@id": "qoo:capa_temp_sensor01",
          "ssn:hasMeasurementProperty": [
            {
              "@id": "qoo:range_temp_sensor01",
              "@type": "ssn:MeasurementRange",
              "qoo:hasMinValue": "-50.0",
              "qoo:hasMaxValue": "+50.0"
            }
          ],
          "ssn:forProperty": {
            "@id": "qoo:temperature"
          }
        }
      ]
    },
    {
      "@id": "qoo:sensor02",
      "@type": "ssn:Sensor",
      "iot-lite:id": "sensor02",
      "geo:location": {
        "@id": "qoo:loc_portet"
      },
      "qoo:sensorStateValue": "CONNECTED",
      "ssn:madeObservation": [
        {
          "@id": "qoo:obs_sensor02",
          "ssn:observedProperty": [
            {
              "@id": "qoo:visibility",
              "@type": "ssn:Property",
              "qoo:canBeRetrievedThrough": {
                "@id": "qoo:serv_sensor02_visib"
              }
            }
          ]
        }
      ],
      "iot-lite:exposedBy": [
        {
          "@id": "qoo:serv_sensor02_visib"
        }
      ],
      "ssn:hasMeasurementCapability": [
        {
          "@id": "qoo:capa_visib_sensor02",
          "ssn:hasMeasurementProperty": [
            {
              "@id": "qoo:range_visib_sensor02",
              "@type": "ssn:MeasurementRange",
              "qoo:hasMinValue": "0.0",
              "qoo:hasMaxValue": "+INF"
            }
          ],
          "ssn:forProperty": {
            "@id": "qoo:visibility"
          }
        }
      ]
    },
    {
      "@id": "qoo:sensor03",
      "@type": "ssn:Sensor",
      "iot-lite:id": "sensor03",
      "geo:location": {
        "@id": "qoo:loc_jardin"
      },
      "qoo:sensorStateValue": "CONNECTED",
      "ssn:madeObservation": [
        {
          "@id": "qoo:obs_sensor03",
          "ssn:observedProperty": [
            {
              "@id": "qoo:visibility",
              "@type": "ssn:Property",
              "qoo:canBeRetrievedThrough": {
                "@id": "qoo:serv_sensor03_visib"
              }
            }
          ]
        }
      ],
      "iot-lite:exposedBy": [
        {
          "@id": "qoo:serv_sensor03_visib"
        }
      ],
      "ssn:hasMeasurementCapability": [
        {
          "@id": "qoo:capa_visib_sensor03",
          "ssn:hasMeasurementProperty": [
            {
              "@id": "qoo:range_visib_sensor03",
              "@type": "ssn:MeasurementRange",
              "qoo:hasMinValue": "0.0",
              "qoo:hasMaxValue": "+INF"
            }
          ],
          "ssn:forProperty": {
            "@id": "qoo:visibility"
          }
        }
      ]
    },
    {
      "@id": "qoo:iQAS",
      "@type": "ssn:Platform",
      "ssn:attachedSystem": [
        {
          "@id": "qoo:sensor01"
        },
        {
          "@id": "qoo:sensor02"
        },
        {
          "@id": "qoo:sensor03"
        }
      ],
      "qoo:considers": [
        {
          "@id": "qoo:OBS_ACCURACY",
          "@type": "qoo:QoOAttribute",
          "qoo:shouldBe": "HIGH"
        },
        {
          "@id": "qoo:OBS_FRESHNESS",
          "@type": "qoo:QoOAttribute",
          "qoo:shouldBe": "LOW"
        },
        {
          "@id": "qoo:OBS_RATE",
          "@type": "qoo:QoOAttribute",
          "qoo:shouldBe": "HIGH"
        }
      ],
      "qoo:provides": [
        {
          "@id": "qoo:SimpleFilteringPipeline",
          "@type": "qoo:QoOPipeline",
          "qoo:in_ObsLevel": "RAW_DATA",
          "qoo:out_ObsLevel": "RAW_DATA",
          "qoo:supports": [

          ],
          "qoo:allowsToSet": [
            {
              "@id": "qoo:threshold_min",
              "@type": "qoo:QoOCustomizableParameter",
              "qoo:documentation": "A string (float) that indicates the lowest admissible value.",
              "qoo:has": [
                {
                  "@id": "qoo:SimpleFilteringPipeline_threshold_min_effect_1",
                  "@type": "qoo:QoOEffect",
                  "qoo:capabilityVariation": "CONSTANT",
                  "qoo:qooAttributeVariation": "HIGH",
                  "qoo:impacts": [
                    {
                      "@id": "qoo:OBS_ACCURACY"
                    }
                  ]
                },
                {
                  "@id": "qoo:SimpleFilteringPipeline_threshold_min_effect_2",
                  "@type": "qoo:QoOEffect",
                  "qoo:capabilityVariation": "CONSTANT",
                  "qoo:qooAttributeVariation": "CONSTANT",
                  "qoo:impacts": [
                    {
                      "@id": "qoo:OBS_FRESHNESS"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "qoo:allowsToSet": [
        {
          "@id": "qoo:obsRate_min",
          "@type": "qoo:QoOCustomizableParameter",
          "qoo:documentation": "A string that indicates the lowest admissible observation rate under the format int/unit. Accepted units: s, min, hour. E.g., '1/s', '3/hour'",
          "qoo:has": [
            {
              "@id": "qoo:General_obsRate_min_effect_1",
              "@type": "qoo:QoOEffect",
              "qoo:capabilityVariation": "CONSTANT",
              "qoo:qooAttributeVariation": "CONSTANT",
              "qoo:impacts": [
                {
                  "@id": "qoo:OBS_RATE"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "qoo:allowsToSet": [
        {
          "@id": "qoo:obsRate_max",
          "@type": "qoo:QoOCustomizableParameter",
          "qoo:documentation": "A string that indicates the maximum admissible observation rate under the format int/unit. Accepted units: s, min, hour. E.g., '1/s', '3/hour'. If both obsRate_min and obsRate_max are specified, only obsRate_max is considered.",
          "qoo:has": [
            {
              "@id": "qoo:General_obsRate_max_effect_1",
              "@type": "qoo:QoOEffect",
              "qoo:capabilityVariation": "CONSTANT",
              "qoo:qooAttributeVariation": "CONSTANT",
              "qoo:impacts": [
                {
                  "@id": "qoo:OBS_RATE"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}