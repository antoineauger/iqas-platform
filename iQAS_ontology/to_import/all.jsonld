{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "iot-lite": "http://purl.oclc.org/NET/UNIS/fiware/iot-lite#",
    "ssn": "http://purl.oclc.org/NET/ssnx/ssn#",
    "qoo": "http://isae.fr/iqas/qoo-ontology#",
    "geo": "http://www.w3.org/2003/01/geo/wgs84_pos#"
  },
  "@graph": [
    {
      "@id": "qoo:temperature",
      "@type": "ssn:Property"
    },
    {
      "@id": "qoo:visibility",
      "@type": "ssn:Property"
    },
    { 
      "@id": "qoo:loc_toulouse",
      "@type": "geo:Point",
      "geo:lat": "43.601176",
      "geo:long": "1.439363",
      "geo:alt": "0",
      "iot-lite:relativeLocation": "Toulouse",
      "iot-lite:altRelative": "0"
    },
    { 
      "@id": "qoo:loc_portet",
      "@type": "geo:Point",
      "geo:lat": "43.53101809",
      "geo:long": "1.40158296",
      "geo:alt": "152",
      "iot-lite:relativeLocation": "Chullanka - Portet-sur-Garonne",
      "iot-lite:altRelative": "0",
      "qoo:isInTheAreaOf": {
        "@id": "qoo:loc_toulouse"
      }
    },
    { 
      "@id": "qoo:loc_jardin",
      "@type": "geo:Point",
      "geo:lat": "43.59543567",
      "geo:long": "1.45255566",
      "geo:alt": "145",
      "iot-lite:relativeLocation": "Jardin des plantes - Toulouse",
      "iot-lite:altRelative": "0",
      "qoo:isInTheAreaOf": {
        "@id": "qoo:loc_toulouse"
      }
    },
    {
      "@id": "qoo:serv_sensor01_temp",
      "@type": "iot-lite:Service",
      "iot-lite:interfaceType": "REST",
      "iot-lite:endpoint": "http://127.0.0.1:9091/s01",
      "iot-lite:interfaceDescription": "First service to get temperature measurements from s01."
    },
    {
      "@id": "qoo:serv_sensor01_visib",
      "@type": "iot-lite:Service",
      "iot-lite:interfaceType": "REST",
      "iot-lite:endpoint": "http://127.0.0.1:9092/s01",
      "iot-lite:interfaceDescription": "Second service to get visibility measurements from s01."
    },
    {
      "@id": "qoo:serv_sensor02_visib",
      "@type": "iot-lite:Service",
      "iot-lite:interfaceType": "REST",
      "iot-lite:endpoint": "http://127.0.0.1:9093/s02",
      "iot-lite:interfaceDescription": "Third service to get visibility measurements from s02."
    },
    {
      "@id": "qoo:sensor01",
      "@type": "ssn:Sensor",
      "iot-lite:id": "sensor01",
      "geo:location": {
        "@id": "qoo:loc_portet"
      },
      "qoo:sensorStateValue": "CONNECTED",
      "ssn:madeObservation": [
        {
          "@id": "qoo:obs_sensor01",
          "ssn:observedProperty": [
            {
              "@id": "qoo:temperature",
              "@type": "ssn:Property",
              "qoo:canBeRetrievedThrough": {
                "@id": "qoo:serv_sensor01_temp"
              }
            },
            {
              "@id": "qoo:visibility",
              "@type": "ssn:Property",
              "qoo:canBeRetrievedThrough": {
                "@id": "qoo:serv_sensor01_visib"
              }
            }
          ]   
        }
      ],
      "iot-lite:exposedBy": [
        {
          "@id": "qoo:serv_sensor01_temp"
        },
        {
          "@id": "qoo:serv_sensor01_visib"
        }
      ],
      "ssn:hasMeasurementCapability": [
        {
          "@id": "qoo:capa_temp_sensor01",
          "ssn:hasMeasurementProperty": [
            {
              "@id": "qoo:range_temp_sensor01",
              "@type": "ssn:MeasurementRange",
              "qoo:hasMinValue": "-50",
              "qoo:hasMaxValue": "50"
            }
          ],
          "ssn:forProperty": {
            "@id": "qoo:temperature"
          }
        },
        {
          "@id": "qoo:capa_visib_sensor01",
          "ssn:hasMeasurementProperty": [
            {
              "@id": "qoo:range_temp_sensor01",
              "@type": "ssn:MeasurementRange",
              "qoo:hasMinValue": "0",
              "qoo:hasMaxValue": "+INF"
            }
          ],
          "ssn:forProperty": {
            "@id": "qoo:visibility"
          }
        }
      ]
    },
    {
      "@id": "qoo:sensor02",
      "@type": "ssn:Sensor",
      "iot-lite:id": "sensor02",
      "geo:location": {
        "@id": "qoo:loc_jardin"
      },
      "qoo:sensorStateValue": "CONNECTED",
      "ssn:madeObservation": [
        {
          "@id": "qoo:obs_sensor02",
          "ssn:observedProperty": [
            {
              "@id": "qoo:visibility",
              "@type": "ssn:Property",
              "qoo:canBeRetrievedThrough": {
                "@id": "qoo:serv_sensor02_visib"
              }
            }
          ]
        }
      ],
      "iot-lite:exposedBy": [
        {
          "@id": "qoo:serv_sensor02_visib"
        }
      ],
      "ssn:hasMeasurementCapability": [
        {
          "@id": "qoo:capa_visib_sensor02",
          "ssn:hasMeasurementProperty": [
            {
              "@id": "qoo:range_visib_sensor02",
              "@type": "ssn:MeasurementRange",
              "qoo:hasMinValue": "0",
              "qoo:hasMaxValue": "+INF"
            }
          ],
          "ssn:forProperty": {
            "@id": "qoo:visibility"
          }
        }
      ]
    },
    {
      "@id": "qoo:iQAS",
      "@type": "ssn:Platform",
      "ssn:attachedSystem": [
        {
          "@id": "qoo:sensor01"
        },
        {
          "@id": "qoo:sensor02"
        }
      ],
      "qoo:considers": [
        {
          "@id": "qoo:OBS_ACCURACY",
          "@type": "qoo:QoOAttribute",
          "shouldBe": {
            "@id": "qoo:HIGH"
          }
        },
        {
          "@id": "qoo:OBS_FRESHNESS",
          "@type": "qoo:QoOAttribute",
          "shouldBe": {
            "@id": "qoo:LOW"
          }
        }
      ],
      "qoo:provides": [
        {
          "@id": "qoo:SimpleFilteringPipeline",
          "@type": "qoo:QoOPipeline",
          "qoo:in_ObsLevel": "RAW_DATA",
          "qoo:out_ObsLevel": "RAW_DATA",
          "qoo:supports": [

          ],
          "qoo:allowsToSet": [
            {
              "@id": "qoo:threshold",
              "@type": "qoo:QoOCustomizableParameter"
            }
          ]
        }
      ]
    }
  ]
}